# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

# this is mainly a javascript project, so we only enforce style on these files
[*.js]
# Unix-style newlines with a newline ending every file
end_of_line = lf
insert_final_newline = false
# Set default charset
charset = utf-8
# 4 space indentation
indent_style = space
indent_size = 4

